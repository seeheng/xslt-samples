function replaceValues(str,key,val){var arr=str.split(';');var done=false;for(var i in arr){var entry=arr[i].split('=');if(entry[0]==key){entry[1]=val;arr[i]=entry.join('=');done=true;}}
str=arr.join(';');if(!done){str+=';'+key+'='+val;}
return str;}
